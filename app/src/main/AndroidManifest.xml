<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="app.Clients_Management.com">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".Clients_Management"
        >
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Login"/>
        <activity android:name=".ClientsList"></activity>
        <activity android:name=".AddClients"></activity>
        <activity android:name=".ClientsDetails"></activity>
        <activity android:name=".ClientsPaid"></activity>
        <activity android:name=".ValidClients"></activity>
        <activity android:name=".AddUser"></activity>
    </application>

</manifest>